//- partials are not supported in Express 3, so we simply loop through the rows. 
extends layout

block content
  h1= title
  p Welcome to #{title}

  - var headers = ['Name', 'Irc', 'Twitter', 'Github', 'Location', 'Description'];

  table#profiles
    tr
      each header in headers
        th= header
      each profile, id in profiles
        tr(id=id)
          each row in profile
            td= row
